<div>
	<table class="tableForm"
		style="margin-left: auto; margin-right: auto; width: 300px;">
		<tr>
			<th width="30%;" align="right">用户名:</th>
			<td><label id="userName2"></label></td>
		</tr>
		<tr>
			<th align="right">IP地址:</th>
			<td><label id="ipAddress2"></label></td>
		</tr>
		<tr>
			<th align="right">用户类型:</th>
			<td><label id="userType2"></label></td>
		</tr>
		<tr>
			<th align="right" valign="top">拥有权限:</th>
			<td><div style="height: 200px; overflow: auto;">
					<ul id="qx"></ul>
				</div></td>
		</tr>
	</table>
	<script type="text/javascript" charset="utf-8">
	$(function()
	{
	  $.ajax({
				type : 'POST',
				url : './ajax/getSessionInfo',
				success : function(result) { 
				    $('#userName2').html(result.userName);
				    $('#ipAddress2').html(result.ipAddress);
				    $('#userType2').html(result.userType);
				    $('#qx').tree(
				      {
				        url:'./ajax/tree_menu_getdata?userTypeId=' + result.userTypeId
			          });
				},
				dataType:'json'
			  });
	  
	});
    </script>
</div>